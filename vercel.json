{
  "version": 2,
  "builds": [
    {
      "src": "api/check.php",
      "use": "@vercel/php"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/check",
      "dest": "/api/check.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}